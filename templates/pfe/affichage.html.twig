{% extends 'layout.html.twig' %}
{% block body %}
    <div class="row">
        <a href="{{ path('app_entreprise') }}"></a>
        {% for entreprise in entreprises %}
            <div class="card mb-2 m-lg-2" style="width: 18rem">
                <div class="card-body">
                    <h5 class="card-title">{{ Entreprise.id }}</h5>
                    <h6 class="card-subtitle mb-2 text-muted">{{ pfes|length  }}</h6>
                </div>
            </div>
        {% endfor %}
    </div>
{% endblock %}